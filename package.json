{
  "name": "n2words",
  "version": "1.7.0",
  "description": "Convert numbers to words, in multiple languages",
  "main": "dist/n2words.min.js",
  "scripts": {
    "lint": "eslint --config .eslintrc.json src/ test/ --ext=mjs",
    "lint:fix": "eslint --config .eslintrc.json src/ test/ --ext=mjs --fix",
    "test": "ava --verbose",
    "coverage": "c8 ava",
    "build": "webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forzagreen/n2words.git"
  },
  "keywords": [
    "n2words",
    "convert",
    "number",
    "word",
    "text",
    "natural",
    "language",
    "english",
    "french",
    "spanish",
    "german",
    "portuguese",
    "italian",
    "turkish",
    "russian",
    "czech",
    "norwegian",
    "danish",
    "polish",
    "ukrainian",
    "lithuanian",
    "latvian",
    "arabic",
    "hebrew",
    "korean",
    "dutch",
    "serbian"
  ],
  "author": "Wael TELLAT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/forzagreen/n2words/issues"
  },
  "homepage": "https://github.com/forzagreen/n2words#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "ava": "^3.7.0",
    "babel-loader": "^8.1.0",
    "c8": "^7.1.0",
    "core-js": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-ava": "^10.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^23.0.0",
    "eslint-plugin-node": "^11.1.0",
    "esm": "^3.2.25",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "ava": {
    "files": [
      "test/**/*",
      "!test/i18n"
    ],
    "require": [
      "esm"
    ]
  },
  "c8": {
    "all": true,
    "include": [
      "src/"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
